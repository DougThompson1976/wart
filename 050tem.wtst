(wt-eval '(deftem t1 a "abc" b "def"))
(test-wart "inst uses templates for per-key defaults"
  :valueof (iso (inst 't1 'b 34 'd 42)
                (obj a "abc" b 34 d 42))
  :should be t)

(test-wart "templates serialize to plain tables"
  :valueof (tostring (write (inst 't1 'a 2 'c 9)))
  :should be "(tagged hash-table ((c 9) (b \"def\") (a 2)))")
