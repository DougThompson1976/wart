(test-wart "pairp works"
  :valueof (pairp '(1 2))
  :should satisfy true_value)

(test-wart "call works on simple functions"
  :valueof (call #'car '(1))
  :should be 1)

(test-wart "call works on simple functions"
  :valueof (call #'car '(1))
  :should be 1)

(test-wart "call coerces to function when necessary"
  :valueof (call 'car '(1))
  :should be 1)

(test-wart "call works on lambdas"
  :valueof (call [+ _ 1] 3)
  :should be 4)
