def test(msg expr/valueof pred/should expected)
  if (pred expr expected)
    (pr ",")
    (do
      inc_failures.
      prn.
      (prn "F " msg)
      (prn "  got " expr))

def pending-test'(msg expr/valueof pred/should expected)
  prn.
  (prn "pending: " msg)

alias be iso
alias exceed >
