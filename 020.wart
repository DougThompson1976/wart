assign cons? _isCons

assign nil? _isNil
assign not nil?
assign no nil?

assign mac (lambda '(name params . body)
  (eval `(assign ,name (lambda ',params
                         (eval ((lambda() ,@body)))))))

mac def(name params . body)
  `(assign ,name (lambda ,params ,@body))

def cadr(x)
  (car (cdr x))

def cddr(x)
  (cdr (cdr x))
