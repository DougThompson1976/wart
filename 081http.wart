(def http-serve(? port 8080)
  (w/server-socket s port
    (repeat 6
      (fork (thunk
              (accepting stream :from s
                (w/stdout stream
                  (dispatch (parse-http-request ip stream)))))))
    (wait-for-children)))

(defpath /foo(req)
  (prn "bar"))

(defpath "/user/:"(req user)
  (prn "user is " user))

;(http-serve)
