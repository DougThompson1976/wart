(test-wart "new redefined let"
  :valueof (let a 3 a)
  :should be 3)

(test-wart "let destructures"
  :valueof (let (a b) '(3 4) a)
  :should be 3)

(test-wart "new keyword - with"
  :valueof (with (a 3) a)
  :should be 3)

(test-wart "map works"
  :valueof (map #'1+ '(2 3 4))
  :should be '(3 4 5))
