(test "tables work in function position"
 :valueof (let h (table)
            (table_set h 3 4)
            h.3)
 :should be 4)

(test "get on tables takes a default"
 :valueof (let h (table)
            (h 3 5))
 :should be 5)

(test "tables can be assigned to"
 :valueof (let h (table)
            (= h.3 4)
            h.3)
 :should be 4)

(test "each works on tables"
 :valueof (ret ans 0
            (each (k v) (table 1 2 3 4)
              ++.ans))
 :should be 2)
