(test "str works with multiple args"
 :valueof (str 'a 43)
 :should be "a43")

(test "sym works with one arg"
 :valueof (sym "abc")
 :should be 'abc)

(test "sym works with multiple args"
 :valueof (sym "abc" 42 'def)
 :should be 'abc42def)

(test "keyword? works"
 :valueof (keyword? 'abc)
 :should be nil)

(test "keyword? works - 2"
 :valueof (keyword? :abc)
 :should ~be nil)
