def (test msg (| 'qpred pred should) expr|valueof expected)
  if (pred expr expected)
    (err ".")
    (register_failed_test msg expr `(,qpred _ ,expected))

def (pending_test 'msg 'expr|valueof 'pred|should 'expected)
  (err "
pending: ")
  (err msg)

alias be (=)
alias equal (=)

def (match a b)
  (match? b a)
