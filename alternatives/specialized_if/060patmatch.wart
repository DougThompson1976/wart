mac (matching vars vals ... body)
  `(if2 (match? ',vars ,vals)
     (let ,strip_leading_underscores.vars ,vals
      ,@body))



## internals

def (strip_leading_underscores vars)
  (if
    no.vars
      vars
    list?.vars
      (cons strip_leading_underscores:car.vars
            strip_leading_underscores:cdr.vars)
    (string.vars.0 = "_")
      (sym (string.vars 1 nil))
    :else
      vars)
