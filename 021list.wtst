(test-wart "list indexing works"
  :valueof (call '(1 2 3) 2)
  :should be 3)

(test-wart "writing to list index works"
  :valueof (ret l '(1 2 3)
             (= l.1 34))
  :should be '(1 34 3))

(test-wart "last has arc semantics rather than common lisp semantics"
  :valueof (last '(1 2 3))
  :should be 3)
