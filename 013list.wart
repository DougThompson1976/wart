(defcall cons l
  [elt l _])

(defset cons(l index val)
  (setf (elt l index)
        val))

(def wart-last(xs)
  (if (cdr xs)
    (wart-last cdr.xs)
    (car xs)))
(defover last wart-last)

(def pos(test s)
  (position-if testify.test s))
(def rpos(test s)
  (position-if testify.test s :from-end t))
